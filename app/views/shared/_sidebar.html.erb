<% sidebar_collapsed = Current.user&.sidebar_collapsed == true %>
<aside data-sidebar-target="sidebar"
      class="fixed top-9 bottom-0 left-0 z-40 flex w-56 shrink-0 -translate-x-full flex-col overflow-hidden duration-200 lg:static lg:top-auto lg:bottom-auto lg:translate-x-0 lg:h-full lg:max-h-full lg:min-h-0 sidebar-chrome <%= sidebar_collapsed ? 'sidebar-mini' : 'sidebar-expanded' %>">
  <div class="flex min-h-0 min-w-0 flex-1 flex-col overflow-hidden">
    <%# ── Brand ── %>
    <div class="sidebar-brand flex shrink-0 items-center justify-between gap-2 px-3 py-2.5 lg:min-h-[44px]">
      <div class="flex min-w-0 flex-1 items-center gap-2.5 overflow-hidden">
        <div class="logo-easter-egg-logo flex h-7 w-7 shrink-0 cursor-default items-center justify-center rounded-md bg-accent text-[10px] font-bold text-[color:var(--color-page)] select-none"
             data-controller="logo-easter-egg"
             data-action="click->logo-easter-egg#click keydown->logo-easter-egg#click"
             role="button"
             tabindex="0"
             aria-label="EI logo">EI</div>
        <div class="sidebar-text min-w-0 flex-1 overflow-hidden">
          <p class="truncate text-[13px] font-semibold text-[var(--sidebar-text-bright)] leading-tight">Point of Sale</p>
          <p class="truncate text-[11px] text-[var(--sidebar-text-muted)] leading-tight"><%= Current.user&.name.presence || Current.user&.email_address %></p>
        </div>
      </div>
      <button type="button"
              class="flex h-6 w-6 items-center justify-center rounded text-[var(--sidebar-text-muted)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text)] lg:hidden"
              data-action="click->sidebar#close"
              aria-label="Close menu">
        <svg class="h-3.5 w-3.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"/>
        </svg>
      </button>
    </div>

    <%# ── Navigation ── %>
    <nav class="sidebar-nav min-h-0 flex-1 overflow-y-auto px-2.5 pb-1">
      <p class="sidebar-section-label mb-0.5 mt-2 px-2 text-[10px] font-semibold uppercase tracking-wider text-[var(--sidebar-text-muted)] opacity-70">
        <span class="sidebar-text">Main</span>
      </p>

      <%= link_to root_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(root_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(root_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Dashboard</span>
      <% end %>

      <% register_draft_count = Order.draft.count %>
      <% register_held_count = Order.held.count %>
      <%= link_to register_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(register_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(register_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M9 7h6m0 10v-3m-3 3h.01M9 17h.01M9 14h.01M12 14h.01M15 11h.01M12 11h.01M9 11h.01M7 21h10a2 2 0 002-2V5a2 2 0 00-2-2H7a2 2 0 00-2 2v14a2 2 0 002 2z"/>
        </svg>
        <span class="sidebar-text min-w-0 flex-1 truncate">Register</span>
        <% if register_draft_count > 0 || register_held_count > 0 %>
          <span class="sidebar-text flex items-center gap-1">
            <% if register_draft_count > 0 %>
              <span class="flex h-4 min-w-4 items-center justify-center rounded-full bg-blue-500 px-1 text-[9px] font-bold text-white"><%= register_draft_count %></span>
            <% end %>
            <% if register_held_count > 0 %>
              <span class="flex h-4 min-w-4 items-center justify-center rounded-full bg-yellow-500 px-1 text-[9px] font-bold text-white"><%= register_held_count %></span>
            <% end %>
          </span>
        <% end %>
      <% end %>

      <%= link_to products_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(products_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(products_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10M4 7v10l8 4"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Products</span>
      <% end %>

      <%= link_to services_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(services_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(services_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M21 13.255A23.931 23.931 0 0112 15c-3.183 0-6.22-.62-9-1.745M16 6V4a2 2 0 00-2-2h-4a2 2 0 00-2 2v2m4 6h.01M5 20h14a2 2 0 002-2V8a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Services</span>
      <% end %>

      <%= link_to customers_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(customers_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(customers_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Customers</span>
      <% end %>

      <%= link_to cash_drawer_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(cash_drawer_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(cash_drawer_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M17 9V7a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2m2 4h10a2 2 0 002-2v-6a2 2 0 00-2-2H9a2 2 0 00-2 2v6a2 2 0 002 2zm7-5a2 2 0 11-4 0 2 2 0 014 0z"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Cash Drawer</span>
      <% end %>

      <%= link_to orders_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(orders_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(orders_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Order History</span>
      <% end %>

      <%= link_to store_tasks_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(store_tasks_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(store_tasks_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-6 9l2 2 4-4"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Store Tasks</span>
      <% end %>

      <%= link_to reports_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(reports_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(reports_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Reports</span>
      <% end %>

      <% if Current.user.is_a?(Admin) %>
        <div class="mt-3 mb-0.5 border-t border-[var(--sidebar-border)] pt-2">
          <p class="sidebar-section-label px-2 text-[10px] font-semibold uppercase tracking-wider text-[var(--sidebar-text-muted)] opacity-70">
            <span class="sidebar-text">Manage</span>
          </p>
        </div>

        <% admin_link_active = nav_section_active?("/admin") && !nav_section_active?(admin_gift_certificates_path) %>
        <%= link_to admin_settings_path,
            data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
            class: [
              "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
              (admin_link_active ?
                "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
                "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
            ].join(" ") do %>
          <svg class="h-4 w-4 shrink-0 <%= admin_link_active ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"/>
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
          </svg>
          <span class="sidebar-text min-w-0 truncate">Admin</span>
        <% end %>

        <%= link_to admin_gift_certificates_path,
            data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
            class: [
              "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
              (nav_section_active?(admin_gift_certificates_path) ?
                "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
                "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
            ].join(" ") do %>
          <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(admin_gift_certificates_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M12 8v13m0-13V6a2 2 0 112 2h-2zm0 0V5.5A2.5 2.5 0 109.5 8H12zm-7 4h14M5 12a2 2 0 110-4h14a2 2 0 110 4M5 12v7a2 2 0 002 2h10a2 2 0 002-2v-7"/>
          </svg>
          <span class="sidebar-text min-w-0 truncate">Gift Certificates</span>
        <% end %>
      <% end %>
    </nav>

    <%# ── Bottom ── %>
    <div class="sidebar-bottom shrink-0 border-t border-[var(--sidebar-border)] px-2.5 py-1.5">
      <div class="relative hidden lg:block">
        <button type="button"
                data-action="click->notifications#toggleDropdown"
                class="nav-link group flex h-[30px] w-full items-center gap-2.5 rounded-md px-2 text-[13px] font-medium text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]"
                aria-label="Notifications">
          <div class="relative">
            <svg class="h-4 w-4 shrink-0 opacity-50 group-hover:opacity-80" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.158V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.159c0 .538-.214 1.055-.595 1.436L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9"/>
            </svg>
            <% desktop_unread = current_user.notifications.persistent.unread.count %>
            <span data-notifications-target="badge"
                  class="absolute -top-1 -right-1.5 flex h-3.5 min-w-3.5 items-center justify-center rounded-full bg-red-500 px-0.5 text-[9px] font-bold text-white <%= 'hidden' if desktop_unread.zero? %>"><%= desktop_unread %></span>
          </div>
          <span class="sidebar-text min-w-0 truncate">Notifications</span>
        </button>

        <%# Notifications popover - initially hidden, shown via Stimulus %>
        <div data-notifications-target="popover"
             class="fixed hidden z-[100] rounded-lg border border-theme bg-surface shadow-xl overflow-hidden"
             style="max-height: 24rem; width: 20rem;">
          <%= turbo_frame_tag "notifications_popover", src: notifications_path, loading: "lazy" do %>
            <div class="p-4 text-center text-sm text-muted">Loading...</div>
          <% end %>
        </div>
      </div>

      <%= link_to edit_profile_path,
          data: { turbo_prefetch: true, action: "mousedown->sidebar#navigate" },
          class: [
            "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium",
            (nav_section_active?(edit_profile_path) ?
              "bg-[var(--sidebar-active)] text-[var(--sidebar-text-bright)]" :
              "text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]")
          ].join(" ") do %>
        <svg class="h-4 w-4 shrink-0 <%= nav_section_active?(edit_profile_path) ? 'opacity-100' : 'opacity-50 group-hover:opacity-80' %>" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"/>
        </svg>
        <span class="sidebar-text min-w-0 truncate">Profile</span>
      <% end %>

      <% if Rails.env.development? %>
        <%= link_to dev_tools_path,
            class: "nav-link group flex h-[30px] items-center gap-2.5 rounded-md px-2 text-[13px] font-medium text-[var(--sidebar-text)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text-bright)]",
            aria: { label: "Dev tools" },
            data: { turbo_prefetch: true } do %>
          <svg class="h-4 w-4 shrink-0 opacity-50 group-hover:opacity-80" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M5 12h14M5 12a2 2 0 01-2-2V6a2 2 0 012-2h14a2 2 0 012 2v4a2 2 0 01-2 2M5 12a2 2 0 00-2 2v4a2 2 0 002 2h14a2 2 0 002-2v-4a2 2 0 00-2-2m-2-4h.01M17 16h.01"/>
          </svg>
          <span class="sidebar-text min-w-0 truncate">Dev tools</span>
        <% end %>
      <% end %>

      <div class="mt-1 flex items-center gap-0.5">
        <button type="button"
                data-action="click->confirm-modal#open"
                class="nav-link group flex h-[28px] flex-1 items-center justify-center gap-2 rounded-md text-[12px] font-medium text-[var(--sidebar-text-muted)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text)]">
          <svg class="h-3.5 w-3.5 shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M17 16l4-4m0 0l-4-4m4 4H7m6 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h4a3 3 0 013 3v1"/>
          </svg>
          <span class="sidebar-text min-w-0 truncate whitespace-nowrap">Sign out</span>
        </button>
        <button type="button"
                data-action="click->sidebar#toggle"
                class="flex h-[28px] w-[28px] shrink-0 items-center justify-center rounded-md text-[var(--sidebar-text-muted)] hover:bg-[var(--sidebar-hover)] hover:text-[var(--sidebar-text)]"
                aria-label="Collapse sidebar">
          <span class="expand-icon hidden">
            <svg class="h-3.5 w-3.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M13 5l7 7-7 7M5 5l7 7-7 7"/>
            </svg>
          </span>
          <span class="collapse-icon">
            <svg class="h-3.5 w-3.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.75" d="M11 19l-7-7 7-7m8 14l-7-7 7-7"/>
            </svg>
          </span>
        </button>
      </div>
    </div>
  </div>
</aside>
